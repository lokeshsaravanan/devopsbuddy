                    <id root="860c5502-1a68-4362-b98b-333cae307f50"/>
                    <code code="55105-1" codeSystem="2.16.840.1.113883.6.1" displayName="OTC - PURPOSE SECTION"/>
                    <title>Purpose</title>
                    <text>
                        <paragraph ID="ID4">Antihistamine</paragraph>
